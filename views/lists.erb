<ul id="lists">
  <% sort_list(@lists) do |list, index| %>
    <li class="<%= completed(list) %>">
      <a href="/lists/<%= index %>">
        <h2><%= list[:name] %></h2> 
        <p><%= incomplete_todos(list) %>/<%= list[:todos].size %></p>
      </a>
    </li>
  <% end %>
</ul>
<% content_for :header_links do %>
  <a class="add" href="/lists/new">New List</a>
<% end %>